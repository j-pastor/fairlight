[sub_resource type="GDScript" id="GDScript_s2ay1"]
script/source = "extends Node2D

func _process(_delta):
	queue_redraw()

func _draw():
	for id in Mainglobal.scene_objects.keys():
		var obj = Mainglobal.scene_objects[id]
		var pos = obj[\"pos\"]
		var size = obj[\"size\"]

		# Punto central de la base
		var P3D = pos + Vector3(size.x / 2.0, size.y / 2.0, 0)
		var P2D = Mainglobal.iso_to_screen(P3D, Mainglobal.origin, Mainglobal.SCALE)


		# Dibujar puntos
		draw_circle(P2D/Mainglobal.SCALE, 4, Color.RED)
"

